<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="296.0" prefWidth="395.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proyectofinal.HelloController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>

   <Label fx:id="welcomeText" text="Bienvenido al Tablero Digital!">
      <font>
         <Font size="26.0" />
      </font></Label>
   <Label text="Usuario">
      <VBox.margin>
         <Insets bottom="-15.0" right="200.0" />
      </VBox.margin>
   </Label>
   <TextField fx:id="TextUsuario">
      <VBox.margin>
         <Insets left="40.0" right="100.0" />
      </VBox.margin>
   </TextField>
   <Label text="Password">
      <VBox.margin>
         <Insets bottom="-15.0" right="200.0" />
      </VBox.margin>
   </Label>
   <PasswordField fx:id="TextContrasena">
      <VBox.margin>
         <Insets left="40.0" right="100.0" />
      </VBox.margin>
   </PasswordField>
   <Button mnemonicParsing="false" onAction="#onIniciarClick" text="Ingresar" />
</VBox>
